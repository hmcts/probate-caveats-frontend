{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "other-names",
  "type": "object",
  "properties": {
    "firstName": {
        "type": "string",
        "minLength": 2,
      "pattern": "^[^<>]+$"
    },
    "lastName": {
      "type": "string",
      "minLength": 2,
      "pattern": "^[^<>]+$"
    }
  },
  "required":[
    "firstName", "lastName"
  ]
}
